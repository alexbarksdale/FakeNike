<script>
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })

    // Parallax Effect
    const parallax = document.getElementById('parallax');

    window.addEventListener('scroll', () => {
        let offset = window.pageYOffset;
        parallax.style.backgroundPositionY = offset * 0.5 + 'px';
    });
